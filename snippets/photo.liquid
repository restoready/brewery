<div class="gallery__item">
  <div class="gallery__img">
    <a href="{{ photo.image | custom_image_url: '1600x1600' }}" class="photo__link venobox" data-gall="myGallery">
      <div class="photo__link-image">
        <img src="{{ photo.image | custom_image_url: '600x350#' }}" alt="{{ photo.caption }}">
        <div class="photo__link-image-overlay"></div>
      </div>
      {% if photo.caption %}
        <span class="photo__caption">{{ photo.caption }}</span>
      {% endif %}
    </a>

  </div>
</div>
